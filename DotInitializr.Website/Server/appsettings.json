{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "DotInitializr": {
    "Templates": [
      {
        "Name": "ASP.NET Core SPA Template",
        "Description": "ASP.NET Core SPA template with Angular or React",
        "SourceType": "git",
        "SourceUrl": "https://github.com/dsuryd/DotInitializr",
        "SourceDirectory": "DotInitializr.UnitTests\\TestTemplate"
      },
      {
        "Name": "Blazor WebAssembly Template",
        "Description": "Blazor WebAssembly App",
        "SourceType": "git",
        "SourceUrl": "https://github.com/dsuryd/DotInitializr",
        "SourceDirectory": "DotInitializr.UnitTests\\TestTemplate_Blazor"
      },
      {
        "Name": "DotNetify React Template",
        "Description": "ASP.NET Core SPA template with React and dotNetify for building real-time web application",
        "SourceType": "git",
        "SourceUrl": "https://github.com/dsuryd/dotNetify-react-template",
        "SourceDirectory": "ReactTemplate\\content"
      },
      {
        "Name": "DotNetify React Hello World",
        "Description": "DotNetify hello world demo using React",
        "SourceType": "git",
        "SourceUrl": "https://github.com/dsuryd/dotNetify",
        "SourceDirectory": "Demo\\React\\HelloWorld.WebPack"
      },
      {
        "Name": "Steeltoe Template (mustache)",
        "Description": "Demo project for Steeltoe",
        "SourceType": "git",
        "SourceUrl": "https://github.com/dsuryd/DotInitializr",
        "SourceDirectory": "DotInitializr.UnitTests\\TestTemplate_Steeltoe"
      },
      {
        "Name": "Steeltoe Template (dotnet)",
        "Description": "Demo project for Steeltoe",
        "SourceType": "git",
        "SourceUrl": "https://github.com/dsuryd/DotInitializr",
        "SourceDirectory": "DotInitializr.UnitTests\\TestTemplate_Steeltoe_DotNet"
      },
      {
        "Name": "Dotnet-Boxed API Template",
        "Description": "Dotnet-Boxed API Template",
        "SourceType": "git",
        "SourceUrl": "https://github.com/Dotnet-Boxed/Templates",
        "SourceDirectory": "Source/ApiTemplate",
        "TemplateType": "dotnet"
      }
    ]
  }
  // example of how to use PAT authentication for the above templates
